<tr>
  <td>
    <%= game_genre.id %>
  </td>
  <td>
    <%= game_genre.name %>
  </td>
  <td>
    <%= l game_genre.created_at, format: :long %>
  </td>
  <td>
    <%= link_to t('defaults.show'), admin_game_genre_path(game_genre), class: 'btn btn-info' %>
    <%= link_to t('defaults.edit'), edit_admin_game_genre_path(game_genre), class: 'btn btn-success' %>
    <%= link_to t('defaults.delete'), admin_game_genre_path(game_genre), method: :delete, data: { confirm: t('defaults.message.delete_confirm') }, class: 'btn btn-danger' %>
  </td>
</tr>
